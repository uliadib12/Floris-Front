---
import Card from "./Card";
import CardContainer from "./CardContainer";
---
<h1 
    id="title" 
    style={{fontFamily: 'Cormorant Garamond sans-serif'}}
    class="text-3xl justify-center font-semibold italic text-pink-500 mb-4">
</h1>
<script>
    document.getElementById("title").innerHTML = document.title;
</script>

<div class="min-h-screen p-4">
    <CardContainer>
        <Card
            id={-1}
            image=""
            title=""
            minPrice={0}
            maxPrice={0}
        />
        <Card
            id={2}
            image="https://images.unsplash.com/photo-1517849845537-4d257902454a?ixlib=rb-1.2.1&auto=format&fit=crop&w=1350&q=80"
            title="Bridal Shower"
            minPrice={20000}
            maxPrice={20000}
        />
        <Card
            id={3}
            image="https://images.unsplash.com/photo-1517849845537-4d257902454a?ixlib=rb-1.2.1&auto=format&fit=crop&w=1350&q=80"
            title="Bridal Shower"
            minPrice={20000}
            maxPrice={30000}
        />
    </CardContainer>
</div>